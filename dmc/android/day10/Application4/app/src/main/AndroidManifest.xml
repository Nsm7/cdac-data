<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sunbeaminfo.application4">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:label="Search" android:name=".activity.SearchActivity"></activity>
        <activity android:name=".activity.LoaderActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.EditContactActivity" />
        <activity
            android:name=".activity.ContactDetailsActivity"
            android:label="Details" />
        <activity
            android:name=".activity.AddContactActivity"
            android:label="Add Contact" />
        <activity
            android:name=".activity.ContactListActivity"
            android:label="Contacts"></activity>
    </application>

</manifest>