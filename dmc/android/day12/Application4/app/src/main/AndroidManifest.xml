<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sunbeaminfo.application4">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.CarDetailsActivity"></activity>
        <activity
            android:name=".activity.UserActivity"
            android:label="Users" />
        <activity
            android:name=".activity.MobileActivity"
            android:label="Mobiles" />
        <activity
            android:name=".activity.CarActivity"
            android:label="Cars" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name=".provider.MyContentProvider"
            android:authorities="com.sunbeaminfo.application4"
            android:exported="true" />
    </application>

</manifest>